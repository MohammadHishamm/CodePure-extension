{
  "name": "codepure",
  "displayName": "CodePure",
  "description": "Code smells detector",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "vscode": "^1.93.0"
  },
  "vsce": {
    "noVerify": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MohammadHishamm/Graduation-Project"
  },
  "icon": "Logo.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python",
    "onLanguage:java"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.activateCommand",
        "title": "CodePure: Activate"
      },
      {
        "command": "extension.deactivateCommand",
        "title": "CodePure: Deactivate"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ ",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "codepure": "file:",
    "tree-sitter": "^0.21.1",
    "tree-sitter-java": "^0.23.4"
  }
}
